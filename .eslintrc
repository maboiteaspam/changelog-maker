{
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": false,
    "builtin": true
  },
  "rules": {
    "block-scoped-var": 0,
    "camelcase": 0,
    "comma-style": [
      2,
      "first"
    ],
    "curly": [
      0,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "global-strict": [
      0,
      "never"
    ],
    "guard-for-in": 0,
    "max-len": [
      2,
      120,
      2
    ],
    "new-cap": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 0,
    "no-loop-func": 2,
    "no-new": 2,
    "no-redeclare": 0,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": false,
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      0,
      "never"
    ],
    "valid-typeof": 2,
    "wrap-iife": [
      0,
      "inside"
    ]
  }
}